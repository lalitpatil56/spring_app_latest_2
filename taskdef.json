{
  "family": "imagedefinitions",
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::897729119076:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "spring-app",
      "image": "<IMAGE1_NAME>",
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp"
        }
      ],
      "essential": true
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "1024",
  "memory": "3072"
}
